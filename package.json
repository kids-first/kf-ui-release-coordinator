{
  "name": "kf-ui-release-coordinator",
  "version": "1.6.0",
  "private": true,
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "3 MB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "1 MB"
    }
  ],
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "apollo-boost": "^0.4.4",
    "auth0-js": "^9.10.0",
    "autoprefixer": "^9.4.3",
    "axios": "^0.18.1",
    "babel-plugin-import": "^1.7.0",
    "draft-js": "^0.10.5",
    "draftjs-to-markdown": "^0.5.1",
    "formik": "^1.5.7",
    "graphql": "^14.5.8",
    "jwt-decode": "^2.2.0",
    "react": "^16.10.2",
    "react-app-rewired": "^2.0.0",
    "react-dom": "^16.10.2",
    "react-draft-wysiwyg": "^1.12.13",
    "react-markdown": "^3.4.1",
    "react-progress-steps": "^0.0.8",
    "react-router-dom": "^4.2.2",
    "react-scripts": "3.2.0",
    "react-timeago": "^4.4.0",
    "semantic-ui-react": "^0.87.1",
    "semver": "^6.3.0",
    "victory": "^30.5.0"
  },
  "scripts": {
    "start": "REACT_APP_COMMITHASH=`git rev-parse --short HEAD` REACT_APP_LAST_VERSION=`git describe --abbrev=0 --tags` react-app-rewired start",
    "build": "REACT_APP_COMMITHASH=`git rev-parse --short HEAD` REACT_APP_LAST_VERSION=`git describe --abbrev=0 --tags` react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
